{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true,
        "serviceworker": true
    },
    "settings": {
        "react": {
            "pgragma": "React",
            "version": "16.7"
        }
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "import",
        "react",
        "prettier"
    ],
    "rules": {
        "no-underscore-dangle": 0,
        "max-len": ["off", {
            "code": 80
        }],
        "no-unused-vars": 0,
        "no-console": 0,
        "indent": [
            0
        ],
        "linebreak-style": [
            "off",
            "eol-last"
        ],
        "quotes": [
            0
        ],
        "semi": [
            0
        ]
    }
}